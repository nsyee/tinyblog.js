<?xml version="1.0" encoding="utf-8" ?><rss version="2.0"><channel><title><%- config.blogName %></title><link>http://<%- config.hostName %></link><description><%- config.description %></description><docs>http://www.rssboard.org/rss-specification</docs><copyright><%- config.copyright %></copyright><webMaster><%- config.email %></webMaster><lastBuildDate><%- lastBuildDate %></lastBuildDate>
<% for (var i=0, sz=recents.length; i<sz; i++) { %>
    <item>
    <author><%- config.authorName %></author>
    <title><%- recents[i].title %></title>
    <link><%- recents[i].url %></link>
    <guid isPermaLink="true"><%- recents[i].url %></gid>
    <description><%- recents[i].body %></description>
    <pubDate><%- recents[i].mtime %></pubDate>
    </item>
<% } %>
</channel>
</rss>
